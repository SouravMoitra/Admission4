%h1
  = @stream.name
.panel.panel-info
  .panel-heading
    General Result
  .panel-body
    .table-responsive
      %table.table-hover.table-custom
        %tr
          %th Rank
          %th ID
        - @gens.each_with_index do |row, index|
          %tr
            %td
              = index + 1
            %td
              = row[0]
