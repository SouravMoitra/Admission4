#order_state_code_wrapper
  - parent_region ||= params[:parent_region]
  - country = Carmen::Country.coded(parent_region)

  - if country.nil?
    %em Please select a country above
  - elsif
    = subregion_select(:address_attributes, :state, parent_region,{}, {class: "form-control", name: "personal[address_attributes][state]"})
  - else
    = text_field(:personal, :state)
